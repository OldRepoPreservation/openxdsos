    <build>
        <defaultGoal>install</defaultGoal>
        <resources>
        </resources>
        <plugins>
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>appfuse-maven-plugin</artifactId>
                <version>${appfuse.version}</version>
                <configuration>
                    <genericCore>${amp.genericCore}</genericCore>
                    <fullSource>${amp.fullSource}</fullSource>
                </configuration>
                <!-- Dependency needed by appfuse:gen-model to connect to database. -->
                <!-- See http://issues.appfuse.org/browse/APF-868 to learn more.    -->
                <dependencies>
                    <dependency>
                        <groupId>${jdbc.groupId}</groupId>
                        <artifactId>${jdbc.artifactId}</artifactId>
                        <version>${jdbc.version}</version>
                    </dependency>
                </dependencies>
            </plugin>
            <plugin>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>2.0.2</version>
                <configuration>
                    <source>1.6</source>
                    <target>1.6</target>
                </configuration>
            </plugin>
            <plugin>
                <artifactId>maven-eclipse-plugin</artifactId>
                <version>2.5.1</version>
                <configuration>
                    <additionalProjectnatures>
                        <projectnature>org.springframework.ide.eclipse.core.springnature</projectnature>
                    </additionalProjectnatures>
                    <additionalBuildcommands>
                        <buildcommand>org.springframework.ide.eclipse.core.springbuilder</buildcommand>
                    </additionalBuildcommands>
                    <downloadSources>true</downloadSources>
                    <downloadJavadocs>true</downloadJavadocs>
                    <wtpversion>1.5</wtpversion>
                </configuration>
            </plugin>
            <plugin>
                <artifactId>maven-idea-plugin</artifactId>
                <version>2.1</version>
                <configuration>
                    <downloadSources>true</downloadSources>
                    <downloadJavadocs>true</downloadJavadocs>
                    <dependenciesAsLibraries>true</dependenciesAsLibraries>
                    <useFullNames>false</useFullNames>
                </configuration>
            </plugin>
			<plugin>
				<artifactId>maven-assembly-plugin</artifactId>
				<configuration>
					<descriptors>
						<descriptor>src/main/assembly/assembly.xml</descriptor>
					</descriptors>
				</configuration>
			</plugin>
        	<plugin>
				<inherited>false</inherited>
				<groupId>com.mycila.maven-license-plugin</groupId>
				<artifactId>maven-license-plugin</artifactId>
				<version>1.5.0</version>
				<configuration>
					<header>${basedir}/src/main/resources/header.txt</header>
					<failIfMissing>true</failIfMissing>
					<aggregate>true</aggregate>							
	                <mapping> 
	                    <jwc>XML_STYLE</jwc> 
	                    <application>XML_STYLE</application> 
	                    <myFileExtension>JAVADOC_STYLE</myFileExtension> 
	                </mapping> 
	                <useDefaultMapping>true</useDefaultMapping> 
					<properties>
						<year>2009-2010</year>
						<company>Misys Open Source Solutions (MOSS) and others</company>
						<contributor1>Misys Open Source Solutions - initial API and implementation</contributor1>
						<contributor2>-</contributor2>
					</properties>
					<excludes>
						<exclude>**/header.txt</exclude>
						<exclude>**/README.txt</exclude>
						<exclude>**/*.log</exclude>
						<exclude>**/*.xml</exclude>
						<exclude>**/*.xsd</exclude>
						<exclude>**/*.properties</exclude>
						<exclude>**/*.bat</exclude>
						<exclude>**/*.list</exclude>
                        <exclude>**/*.mar</exclude>
                        <exclude>**/*.aar</exclude>
                        <exclude>**/.xsdconfig</exclude>
                        <exclude>**/*.original</exclude>
                        <exclude>**/.myhibernatedata</exclude>
                        <exclude>**/axis2.xml</exclude>
						<exclude>**/*.sh</exclude>
						<exclude>**/*.jks</exclude>
						<exclude>**/*.p12</exclude>
						<exclude>**/build/**</exclude>
						<exclude>**/test/resources/**</exclude>
						<exclude>**/gov/nist/**</exclude>
						<exclude>**/org/json/**</exclude>
						<exclude>**/common/ws/server/**</exclude>
						<exclude>**/Utility.java</exclude>
						<exclude>**/ByteArrayInputStream.java</exclude>
					</excludes>
                </configuration>
	            <executions>
	                <execution>
	                    <goals>
	                        <goal>format</goal>
	                    </goals>
	                </execution>
	            </executions>                    
            </plugin>
        </plugins>
        <extensions>
        	<extension>
        		<groupId>com.google.code.maven-svn-wagon</groupId>
        		<artifactId>maven-svn-wagon</artifactId>
                <version>1.1</version>
        	</extension>
        </extensions>
